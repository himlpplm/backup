#:kivy 1.9.0

<MainView@GridLayout>:
	operatelist: d_operatelist
	filemanager: d_filemanager
	statusbar: d_statusbar
	cols: 1
	canvas:
		Color:
			rgba: 1, 1, 1, 1
		Rectangle:
			pos: self.pos
			size: self.size
	OperateList:
		id: d_operatelist
	FileManager:
		id: d_filemanager
	StatusBar:
		id: d_statusbar